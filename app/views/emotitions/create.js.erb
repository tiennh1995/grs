<% if user_signed_in? %>
  $('.review-like').html('<%= j render "emotitions/like",
    review: @review, emotition: @emotition %>');
  $('.review-dislike').html('<%= j render "emotitions/dislike",
    review: @review, emotition: @emotition %>');
<% else %>
  $('#form-login-modal').html('<%= j render "devise/sessions/form_modal" %>');
  $('#form-login-modal').modal('show');
<% end %>
